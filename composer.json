{
    "name": "parable-php/framework",
    "type": "library",
    "description": "Parable PHP Framework is a minimalist framework",
    "keywords": [
        "framework",
        "micro",
        "prototyping",
        "minimal",
        "parable"
    ],
    "homepage": "https://github.com/parable-php/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin de Graaf",
            "email": "hello@devvoh.com",
            "homepage": "https://devvoh.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pcntl": "*",
        "parable-php/console": "^1.0",
        "parable-php/di": "^1.0",
        "parable-php/event": "^1.0",
        "parable-php/getset": "^1.0",
        "parable-php/orm": "^1.0",
        "parable-php/routing": "^1.0",
        "parable-php/http": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Parable\\Framework\\": "src/"
        },
        "files": [
            "src/initialize.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Parable\\Framework\\Tests\\": "tests"
        }
    },
    "bin": [
        "src/parable"
    ]
}
